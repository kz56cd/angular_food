@import url("http://fonts.googleapis.com/css?family=Roboto\+Slab:400,700|Roboto:400,700,700italic,400italic")

html
  font-family 'Roboto Slab', serif

body
  background-color #9f3972

[layout=row]
  flex-direction row

.browsehappy
  margin 0.2em 0
  background #ccc
  color #000
  padding 0.2em 0

.thumbnail
  height 200px
  img
    &.pull-right
      width 50px

md-toolbar
  &.md-default-theme
    background-color #9f3972

section
  &.jumbotron
    margin-bottom 30px
    padding 1px 30px
    background-color #e9bad5
    text-align center
    color white
    h1
      font-size 3em

.techs
  display flex
  flex-flow row wrap
  md-card
    width 30%
    img
      &.pull-right
        float right
        width 50px

.bg-info
  min-width 800px
  width 100%
  p
    text-align center

.foodlist
  margin 0px 0px 30px 0px 
  th
    min-width 250px
    padding 10px 0px 10px 0px
  td
    min-width 250px
    padding 10px 0px 10px 0px

md-content.md-default-theme
  margin 0px auto 0px auto

